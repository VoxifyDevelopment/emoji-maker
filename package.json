{
    "name": "emoji-maker",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "description": "Create Emojis for you Discord server with ease",
    "scripts": {
        "dev": "vite",
        "dev:ex": "vite --host",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "preview:ex": "vite preview --host",
        "prettier": "npx prettier --write .",
        "licenseCheck": "license-check-and-add check -f ./licenseConfig.json",
        "licenseFormat": "license-check-and-add add -f ./licenseConfig.json",
        "test": "echo \"Error: no test specified\" && exit 0"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/VoxifyDevelopment/emoji-maker.git"
    },
    "keywords": [
        "discord",
        "emoji",
        "emoji-maker",
        "discord-emoji",
        "discord-emoji-maker",
        "voicebot"
    ],
    "author": "VoxifyDevelopment <contact@voxify.dev>",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/VoxifyDevelopment/emoji-maker/issues"
    },
    "homepage": "https://github.com/VoxifyDevelopment/emoji-maker#readme",
    "dependencies": {
        "@headlessui/react": "^1.7.18",
        "html-to-image": "^1.11.11",
        "react": "^18.2.0",
        "react-colorful": "^5.6.1",
        "react-dom": "^18.2.0",
        "react-icons": "^5.0.1",
        "react-router-dom": "^6.21.2",
        "react-toastify": "^10.0.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "license-check-and-add": "^4.0.5",
        "postcss": "^8.4.33",
        "prettier": "^3.2.2",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-pwa": "^0.17.4"
    }
}
